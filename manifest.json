{
  "manifest_version": 2,
  "name": "github-project-helper",
  "version": "0.1.1",
  "applications": {
    "gecko": {
      "id": "github-project-helper@mkelly.me"
    }
  },

  "description": "Adds useful shortcuts to the GitHub projects page.",

  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/projects/*"
      ],
      "js": ["moment.js", "index.js"]
    }
  ]
}
