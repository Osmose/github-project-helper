{
  "manifest_version": 2,
  "name": "github-project-helper",
  "version": "0.1.0",

  "description": "Adds useful shortcuts to the GitHub projects page.",

  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/projects/*"
      ],
      "js": ["node_modules/moment/moment.js", "index.js"]
    }
  ]
}
